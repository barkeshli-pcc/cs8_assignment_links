<?xml version="1.0" encoding="UTF-8"?>
<cartridge_basiclti_link xmlns="http://www.imsglobal.org/xsd/imslticc_v1p0" xmlns:blti="http://www.imsglobal.org/xsd/imsbasiclti_v1p0" xmlns:lticm="http://www.imsglobal.org/xsd/imslticm_v1p0" xmlns:lticp="http://www.imsglobal.org/xsd/imslticp_v1p0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.imsglobal.org/xsd/imslticc_v1p0 http://www.imsglobal.org/xsd/lti/ltiv1p0/imslticc_v1p0.xsd&#10;                          http://www.imsglobal.org/xsd/imsbasiclti_v1p0 http://www.imsglobal.org/xsd/lti/ltiv1p0/imsbasiclti_v1p0p1.xsd&#10;                          http://www.imsglobal.org/xsd/imslticm_v1p0 http://www.imsglobal.org/xsd/lti/ltiv1p0/imslticm_v1p0.xsd&#10;                          http://www.imsglobal.org/xsd/imslticp_v1p0 http://www.imsglobal.org/xsd/lti/ltiv1p0/imslticp_v1p0.xsd">
  <blti:title>YuJa Verity</blti:title>
  <blti:description>LTI 1.3 integration for YuJa Verity</blti:description>
  <blti:secure_launch_url>https://zone1.verity.yuja.com/lti/1.3</blti:secure_launch_url>
  <blti:vendor>
    <lticp:code>unknown</lticp:code>
    <lticp:name>unknown</lticp:name>
  </blti:vendor>
  <blti:custom>
    <lticm:property name="user_id">$User.id</lticm:property>
    <lticm:property name="username">$Canvas.user.loginId</lticm:property>
    <lticm:property name="course_id">$Canvas.course.id</lticm:property>
    <lticm:property name="full_name">$Person.name.full</lticm:property>
    <lticm:property name="sis_email">$vnd.Canvas.Person.email.sis</lticm:property>
    <lticm:property name="logout_url">$Canvas.logoutService.url</lticm:property>
    <lticm:property name="course_role">$Canvas.membership.roles</lticm:property>
    <lticm:property name="display_name">$Person.name.display</lticm:property>
    <lticm:property name="primary_email">$Person.email.primary</lticm:property>
    <lticm:property name="profile_picture">$User.image</lticm:property>
  </blti:custom>
  <blti:extensions platform="canvas.instructure.com">
    <lticm:property name="privacy_level">public</lticm:property>
    <lticm:property name="domain">yuja.com</lticm:property>
    <lticm:property name="lti_version">1.3</lticm:property>
    <lticm:property name="client_id">10790000000000392</lticm:property>
    <lticm:property name="client_id">10790000000000392</lticm:property>
    <lticm:property name="platform">canvas.instructure.com</lticm:property>
    <lticm:options name="account_navigation">
      <lticm:property name="enabled">true</lticm:property>
      <lticm:property name="placement">account_navigation</lticm:property>
      <lticm:property name="visibility">admins</lticm:property>
      <lticm:property name="message_type">LtiResourceLinkRequest</lticm:property>
      <lticm:property name="target_link_uri">https://zone1.verity.yuja.com/config/institute.html</lticm:property>
    </lticm:options>
    <lticm:options name="course_navigation">
      <lticm:property name="default">disabled</lticm:property>
      <lticm:property name="enabled">true</lticm:property>
      <lticm:property name="placement">course_navigation</lticm:property>
      <lticm:property name="message_type">LtiResourceLinkRequest</lticm:property>
      <lticm:property name="target_link_uri">https://zone1.verity.yuja.com/download/chrome.html</lticm:property>
    </lticm:options>
    <lticm:options name="quiz_menu">
      <lticm:property name="enabled">true</lticm:property>
      <lticm:property name="placement">quiz_menu</lticm:property>
      <lticm:property name="visibility">admins</lticm:property>
      <lticm:property name="message_type">LtiResourceLinkRequest</lticm:property>
      <lticm:property name="target_link_uri">https://zone1.verity.yuja.com/test-results/test-results.html</lticm:property>
      <blti:custom>
        <lticm:property name="course_id">$Canvas.course.id</lticm:property>
      </blti:custom>
    </lticm:options>
    <lticm:options name="editor_button">
      <lticm:property name="text">YuJa Verity Instructions</lticm:property>
      <lticm:property name="enabled">true</lticm:property>
      <lticm:property name="placement">editor_button</lticm:property>
      <lticm:property name="visibility">admins</lticm:property>
      <lticm:property name="message_type">LtiDeepLinkingRequest</lticm:property>
      <lticm:property name="target_link_uri">https://zone1.verity.yuja.com/pages/instructions/instructions.html</lticm:property>
    </lticm:options>
  </blti:extensions>
</cartridge_basiclti_link>
